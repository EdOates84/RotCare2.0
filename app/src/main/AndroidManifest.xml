    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Req_History_Info_Activity" />
        <activity android:name=".Req_Status_Info_Activity" />
        <activity android:name=".Alloted_Req_Info_Activity" />
        <activity android:name=".Pending_Req_Info_Activity" />
        <activity
            android:name=".Alloted_req_Activity"
            android:label="Accepted Request"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Pending_req_Activity"
            android:label="Pending Request"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Req_history_Activity"
            android:label="Request History"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".V_req_History"
            android:label="Request History"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".My_req_Activity"
