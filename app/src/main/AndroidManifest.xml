            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Apply_for_req_Activity"
            android:label="Apply for Request"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Nav_ContactsActivity"
            android:label="Contacts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Nav_MessageActivity"
            android:label="Message"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Nav_RewardActivity"
            android:label="My Rewards"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Nav_ProfileActivity"
            android:label="My Profile"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>